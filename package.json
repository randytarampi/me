{
  "name": "@randytarampi/posts",
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca"
  },
  "version": "2.6.0-0",
  "private": true,
  "scripts": {
    "pretest": "gulp lint",
    "test": "NODE_ENV=test gulp test --verbose --debug",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test ./node_modules/nyc/bin/nyc.js npm test",
    "travis": "npm run cover; npm run coveralls",
    "coveralls": "cat coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "npm run dev",
    "dev": "npm run dev:serverless",
    "dev:serverless": "serverless offline start --stage local",
    "deploy": "if [ -z \"$NODE_ENV\" ]; then NODE_ENV=dev npm run deploy; else npm run deploy:$NODE_ENV; fi; ",
    "deploy:dev": "sls create_domain --stage $NODE_ENV --region us-east-1 && sls deploy --stage $NODE_ENV --region us-east-1",
    "deploy:prd": "sls create_domain --stage $NODE_ENV --region ca-central-1 && sls deploy --stage $NODE_ENV --region ca-central-1",
    "version": "if [ -z \"$TRAVIS_TAG\" && \"$COMMIT_AUTHOR\" != \"$TRAVIS_AUTHOR\" ]; then npm shrinkwrap && git add --verbose --force --all npm-shrinkwrap.json; fi;",
    "build:flickr": "node node_modules/flickrapi/compile.js",
    "build": "npm run build:flickr"
  },
  "description": "A backend to randytarampi/me.photos, randytarampi/me.words and randytarampi/me.blog.",
  "homepage": "https://github.com/randytarampi/me.posts",
  "bugs": {
    "url": "https://github.com/randytarampi/me.posts/issues",
    "email": "bugs@randytarampi.ca"
  },
  "dependencies": {
    "500px": "^0.5.1",
    "@randytarampi/js": "randytarampi/me.common.js",
    "flickrapi": "^0.6.0",
    "form-data": "^2.3.2",
    "instagram-api": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.12.0",
    "knox": "^0.9.2",
    "lodash": "^4.17.10",
    "lwip": "github:kant2002/lwip#node-10",
    "moment": "^2.22.2",
    "raven": "^2.6.3",
    "redux": "^4.0.0",
    "tumblr.js": "^1.1.1",
    "unsplash-js": "^4.8.0"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-minify-replace": "^0.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "~3.0.1",
    "eslint-plugin-import": "^2.13.0",
    "gulp": "^4.0.0",
    "gulp-eslint": "^4.0.2",
    "gulp-if": "^2.0.2",
    "gulp-mocha": "^6.0.0",
    "habitat": "^3.1.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "serverless-domain-manager": "^2.4.0",
    "serverless-mocha-plugin": "^1.8.3",
    "serverless-offline": "^3.25.4",
    "serverless-secrets": "^3.0.0-beta.12",
    "serverless-webpack": "^5.1.5",
    "vinyl-paths": "^2.1.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=8"
  }
}
