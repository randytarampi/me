{
  "name": "me.blog",
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca"
  },
  "version": "2.4.0",
  "private": true,
  "scripts": {
    "pretest": "gulp lint",
    "test": "NODE_ENV=test gulp test",
    "postinstall": "npm run build",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test ./node_modules/nyc/bin/nyc.js npm test",
    "travis": "npm run cover; npm run coveralls",
    "coveralls": "cat coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "npm run dev",
    "dev": "npm run dev:client & npm run dev:serverless && fg",
    "dev:client": "webpack-serve --hot --config ./webpack.client.config.js",
    "dev:serverless": "serverless offline start --stage local",
    "deploy": "if [ -z \"$NODE_ENV\" ]; then NODE_ENV=dev npm run deploy; else npm run deploy:$NODE_ENV; fi; ",
    "deploy:dev": "sls deploy --stage $NODE_ENV --region us-east-1",
    "deploy:prd": "sls deploy --stage $NODE_ENV --region ca-central-1",
    "version": "gulp docs && git add --verbose --force --all docs",
    "build": "gulp build"
  },
  "description": "A post aggregator/blog thingy. For some usernames at some popular services, pull their posts and display them nicely.",
  "homepage": "https://github.com/randytarampi/me.blog",
  "bugs": {
    "url": "https://github.com/randytarampi/me.blog/issues",
    "email": "bugs@randytarampi.ca"
  },
  "dependencies": {
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "materialize-css": "^0.100.2",
    "me.common.css": "randytarampi/me.common.css",
    "me.common.js": "randytarampi/me.common.js",
    "me.common.jsx": "randytarampi/me.common.jsx",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-materialize": "^2.2.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-minify-replace": "^0.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "config": "^1.30.0",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.8.2",
    "grunt-sass-tilde-importer": "^1.0.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.2",
    "gulp-if": "^2.0.2",
    "gulp-mocha": "^6.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.0.1",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.4.1",
    "koa-compress": "^3.0.0",
    "koa-connect": "^2.0.1",
    "mocha": "^5.2.0",
    "prop-types": "~15.6.0",
    "react-hot-loader": "^4.2.0",
    "serverless-mocha-plugin": "^1.8.3",
    "serverless-offline": "^3.25.5",
    "serverless-secrets": "^3.0.0-beta.12",
    "serverless-webpack": "^5.1.5",
    "style-loader": "^0.21.0",
    "vinyl-paths": "^2.1.0",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^1.0.4"
  },
  "engines": {
    "node": ">=8"
  }
}
