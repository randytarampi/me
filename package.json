  "version": "4.6.0-0",
  "private": true,
  "scripts": {
    "pretest": "npx gulp lint",
    "test": "NODE_ENV=test gulp test",
    "postinstall": "npm run build",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test ./node_modules/nyc/bin/nyc.js npm test",
    "travis": "npm run cover; npm run coveralls",
    "coveralls": "cat coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "npm run dev",
    "dev": "npm run dev:client",
    "dev:client": "webpack-serve --hot --config ./webpack.client.config.js",
    "version": "if [ -z \"$TRAVIS_TAG\" ]; then if [ \"$COMMIT_AUTHOR\" != \"$TRAVIS_AUTHOR\" ]; then npm shrinkwrap && git add --verbose --force --all npm-shrinkwrap.json; fi; fi; gulp docs && git add --verbose --force --all docs;",
    "build": "npx gulp build"
  },
  "description": "A post aggregator/blog thingy. For some usernames at some popular services, pull their posts and display them nicely.",
  "homepage": "https://github.com/randytarampi/me.blog",
  "bugs": {
    "url": "https://github.com/randytarampi/me.blog/issues",
    "email": "bugs@randytarampi.ca"
  },
