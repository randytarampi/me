{
  "name": "@randy.tarampi/jsx",
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca",
    "url": "https://www.randytarampi.ca"
  },
  "version": "2.2.0",
  "description": "Some common JSX components for www.randytarampi.ca",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "pretest": "gulp -LLLL --color lint",
    "test": "NODE_ENV=test gulp -LLLL --color test",
    "build": "npm run build:gulp && npm run build:babel",
    "build:gulp": "gulp -LLLL --color build",
    "build:babel": "BABEL_ENV=publish babel src --out-dir build --root-mode upward --copy-files",
    "prepare": "if [ -z \"$IS_PUBLISHING\" ]; then npm run build; fi;",
    "prepack": "if [ -z \"$RELEASE\" ] && [ -z \"$CI\" ]; then NODE_ENV=prd npm run build; fi; if [ ! -d \"./dist\" ] && [ ! -d \"./build\" ]; then exit 74; fi;",
    "prepublishOnly": "npm shrinkwrap",
    "start": "npm run dev",
    "dev": "npm run dev:client",
    "dev:client": "rm -rf dist/*.js dist/*.css && webpack-serve ./webpack.client.config.js --hotClient",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test nyc gulp -LLLL --color test",
    "clean": "gulp -LLLL --color clean",
    "preuninstall": "npm run clean"
  },
  "bugs": {
    "url": "https://github.com/randytarampi/me/issues",
    "email": "bugs@randytarampi.ca"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/randytarampi/me.git"
  },
  "license": "MIT",
  "keywords": [
    "bear",
    "woof",
    "纬",
    "横糸",
    "씨",
    "subtegmine",
    "гав",
    "υφάδι",
    "نسيج",
    "پود",
    "বুনন",
    "ʕ•ᴥ•ʔ",
    "emoji"
  ],
  "dependencies": {
    "@actra-development-oss/redux-persist-transform-filter-immutable": "^1.0.0",
    "@randy.tarampi/js": "^2.2.0",
    "@randy.tarampi/react-dimensions": "^2.0.0-alpha.3",
    "@randy.tarampi/redux-offline-immutable-config": "latest",
    "@randy.tarampi/schema-dot-org-json-ld-components": "latest",
    "@randy.tarampi/schema-dot-org-types": "latest",
    "@redux-offline/redux-offline": "^2.4.0",
    "browser-bunyan": "^1.4.0",
    "bunyan": "beta",
    "bunyan-sentry-stream": "github:randytarampi/bunyan-sentry-stream#me-release",
    "connected-react-router": "^5.0.0",
