    "is-html": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "luxon": "^1.3.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "raven-js": "^3.26.3",
    "react-hot-loader": "^4.3.3",
    "react-infinite": "^0.13.0",
    "react-metrics": "^2.4.1",
    "react-progressive-image": "^0.6.0",
    "react-router": "^4.3.1",
