  "publishConfig": {
    "access": "public"
  },
  "repository": "randytarampi/me.git",
  "scripts": {
    "build": "npm run build:gulp && npm run build:babel",
    "build:babel": "npm run build:babel:esm && npm run build:babel:es5",
    "build:babel:es5": "BABEL_ENV=client.es5 babel src --out-dir es5 --root-mode upward --copy-files",
    "build:babel:esm": "BABEL_ENV=client.esm babel src --out-dir esm --root-mode upward --copy-files",
    "build:gulp": "gulp -LLLL --color build",
    "clean": "gulp -LLLL --color clean",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test nyc gulp -LLLL --color test",
    "dev": "npm run dev:client",
    "dev:client": "npm run predev:client && webpack-serve ./webpack.client.config.js --hotClient",
    "predev:client": "rm -rf dist/*.js dist/*.css",
    "prepack": "if [ -z \"$RELEASE\" ] && [ -z \"$CI\" ]; then NODE_ENV=prd npm run build; fi; if [ ! -d \"./dist\" ] && [ ! -d \"./es5\" ] && [ ! -d \"./esm\" ]; then exit 74; fi;",
    "prepare": "if [ -z \"$IS_PUBLISHING\" ]; then npm run build; fi;",
    "pretest": "gulp -LLLL --color lint",
    "preuninstall": "npm run clean",
    "start": "npm run dev",
    "test": "NODE_ENV=test gulp -LLLL --color test"
  },
  "sideEffects": false
}
