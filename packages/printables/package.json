{
  "name": "@randy.tarampi/printables",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Randy Tarampi",
    "email": "rt@randytarampi.ca",
    "url": "https://www.randytarampi.ca"
  },
  "version": "2.0.4",
  "description": "A wrapper around some common printable tasks for jsonresume-theme-randytarampi and @randy.tarampi/letter",
  "homepage": "https://www.randytarampi.ca",
  "bugs": {
    "url": "https://github.com/randytarampi/me/issues",
    "email": "bugs@randytarampi.ca"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/randytarampi/me.git"
  },
  "license": "MIT",
  "scripts": {
    "pretest": "gulp -LLLL --color lint",
    "test": "NODE_ENV=test gulp -LLLL --color test",
    "build": "BABEL_ENV=publish babel src --out-dir build --root-mode upward --copy-files",
    "prepare": "if [ -z \"$IS_PUBLISHING\" ]; then npm run build; fi;",
    "prepack": "if [ -z \"$RELEASE\" ] && [ -z \"$CI\" ]; then NODE_ENV=prd npm run build; fi; if [ ! -d \"./dist\" ] && [ ! -d \"./build\" ]; then exit 74; fi;",
    "prepublishOnly": "npm shrinkwrap",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test nyc gulp -LLLL --color test",
    "clean": "gulp -LLLL --color clean",
    "preuninstall": "npm run clean"
  },
  "dependencies": {
    "@randy.tarampi/views": "^2.0.4",
    "exiftool-vendored": "^7.1.0",
    "pug": "^2.0.3",
    "puppeteer": "^1.8.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "gulp": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "engines": {
    "node": ">=8"
  },
  "module": "./index.js",
  "main": "./index.js",
  "directories": {
    "lib": "./src/lib",
    "test": "./test"
  },
  "files": [
    "build",
    "npm-shrinkwrap.json",
    "index.js"
  ]
}
