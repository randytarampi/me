  "scripts": {
    "build": "CAMPAIGN_SOURCE=$(node -p \"require(\\\"./package.json\\\").name\") CAMPAIGN_CONTENT=$(node -p \"require(\\\"./package.json\\\").version\") gulp -LLLL --color build",
    "clean": "gulp -LLLL --color clean",
    "cover": "rm -rf coverage/ .nyc_output/; NODE_ENV=test nyc gulp -LLLL --color test",
    "dev": "npm run dev:client",
    "dev:client": "npm run predev:client && CAMPAIGN_SOURCE=$(node -p \"require(\\\"./package.json\\\").name\") CAMPAIGN_CONTENT=$(node -p \"require(\\\"./package.json\\\").version\") webpack-serve ./webpack.client.config.es5.js --hotClient",
    "docs": "gulp -LLLL --color docs",
    "predev:client": "rm -rf dist/*.js dist/*.css",
    "prepare": "if [ -z \"$IS_PUBLISHING\" ]; then npm run build; fi;",
    "pretest": "gulp -LLLL --color lint",
    "preuninstall": "npm run clean",
    "start": "npm run dev",
    "test": "NODE_ENV=test gulp -LLLL --color test"
  }
