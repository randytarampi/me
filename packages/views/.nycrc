{
  "exclude": [
    "node_modules/**",
    "build/**",
    "coverage/**",
    ".nyc_output/**",
    "test/**",
    "*.js"
  ],
  "reporter": [
    "text-summary",
    "lcov"
  ],
  "require": [
    "../../babel.register.js"
  ],
  "cache": true,
  "all": true,
  "check-coverage": true,
  "sourceMap": false,
  "instrument": false,
  "watermarks": {
    "lines": [
      70,
      90
    ],
    "functions": [
      70,
      90
    ],
    "branches": [
      70,
      90
    ],
    "statements": [
      70,
      90
    ]
  }
}
